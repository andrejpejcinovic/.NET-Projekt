        <div class="col-md-4">
            <h2>Filip Oroz</h2>
            <p>
                @Html.ActionLink("Filip", "Filip", "Home", new { @class = "btn btn-default" })
            </p>
        </div>
        <div class="col-md-4">
            <h2>Klara Kovačić</h2>
            <p>
                @Html.ActionLink("Klara", "Klara", "Home", new { @class = "btn btn-danger" })
            </p>
        </div>
        <div class="col-md-4 ">
            <h2>Antun Lucijan Mandić</h2>
